	<queryString language="SQL">
		<![CDATA[SELECT
     detail_pelunasan_pembayaran.`Id` AS detail_pelunasan_pembayaran_Id,
     detail_pelunasan_pembayaran.`No_Pembayaran` AS detail_pelunasan_pembayaran_No_Pembayaran,
     detail_pelunasan_pembayaran.`No_Transaksi` AS detail_pelunasan_pembayaran_No_Transaksi,
